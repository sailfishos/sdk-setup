UID_MERSDK=1001
GID_MERSDK=1001

# Change fmode to 0755 if you need to execute binaries from source or build
# directory as a build step.  The default 0644 fmode has been chosen to
# eliminate the all-files-executable symptom with MS Windows as the host OS.
FMODE_HOME_MERSDK=0644
DMODE_HOME_MERSDK=0755

# Omitting 'direct_io' is known to cause random read errors!
DYNEXECFS_FLAGS=direct_io
